<apex:page showheader="false" standardStyleSheets="true" controller="SLSI.SL_IntranetPageController"  id="IntranetPage" showChat="{!$Setup.SLSI__SL_CustomizableComponent__c.SLSI__ShowChat__c}">
     
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub,'/IntranetHub/js/jQuery-1.8.js')}" /> 
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub,'/IntranetHub/js/jQueryUI-1.9.js')}" />
    
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub, '/IntranetHub/commonJS/angular.min.js')}" />
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub, '/IntranetHub/commonJS/angular-touch.min.js')}" />
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub, '/IntranetHub/commonJS/moment.min.js')}" />
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub, '/IntranetHub/commonJS/moment-timezone.min.js')}" />
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub, '/IntranetHub/commonJS/angular-sanitize.js')}" />
    
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub, '/IntranetHub/commonJS/PageControllerJS.js')}" />   
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub, '/IntranetHub/commonJS/WidgetConfigJSON.js')}" />
    
    
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub,'/IntranetHub/js/browser.detect.js')}" />
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub,'/IntranetHub/js/hide.chrome.js')}" />
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub,'/IntranetHub/js/match.media.js')}" />
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub,'/IntranetHub/js/respond.js')}"/>
         
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub,'/IntranetHub/js/jcf.js')}" /> 
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub,'/IntranetHub/js/jcf.select.js')}" />
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub,'/IntranetHub/js/jcf.checkbox.js')}" />
    
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub,'/IntranetHub/js/clear.inputs.js')}" />
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub,'/IntranetHub/js/tabs.js')}" />
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub,'/IntranetHub/js/lib.js')}"/>
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub,'/IntranetHub/js/touch.nav.js')}" />
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub,'/IntranetHub/js/child.js')}" />
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub,'/IntranetHub/js/accordion.js')}" />
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub,'/IntranetHub/js/add.focus.class.js')}" />
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub,'/IntranetHub/js/pressed.state.js')}" />
    
    <!-- To detect the availability of flash playre -->
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub,'/IntranetHub/js/swfobject.js')}" />
    <!-- End -->
    
    <!-- Widget js -->
    <apex:repeat value="{!widgetClasses}" var="widget" id="widgetClassesRows">
        <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub, '/IntranetHub/widgetJS/' + widget + '.js')}" />
    </apex:repeat>
    <!-- End -->
    
    <!-- This has been used for chatter plugin. But have been put here so wont be loaded multiple times for different instances -->
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub, '/IntranetHub/widgetJS/SL_IntranetChatterPlugIn.js')}" />
    <apex:includeScript value="{!URLFOR($Resource.SLSI__AutoResize)}" />
    <apex:includeScript value="{!URLFOR($Resource.SLSI__Mention_Autocomplete, '/TriggeredAutocomplete.js')}" /> 
    <apex:includeScript value="{!URLFOR($Resource.SLSI__Mention_Autocomplete, '/FeedItemTopic/jquery.tokeninput.js')}"/> 
    <apex:stylesheet value="{!URLFOR($Resource.SLSI__Mention_Autocomplete, '/FeedItemTopic/token-input.css')}" />
    <apex:styleSheet value="{!URLFOR($Resource.SLSI__Mention_Autocomplete, '/FeedItemTopic/token-input-listStyle.css')}"/>
    <!-- End -->
    
    
    <!-- To establish the connect and for Sforce -->
    <apex:includeScript value="{!URLFOR($Resource.SLSI__SL_IntranetHub,'/IntranetHub/js/apex_Connection.js')}" />
    
    <script>
        var SL_UserTimeZone = '{!userTimeZone}';
    </script>
    <!--End  -->
    
    <!-- Adding styles -->
    
    
    
    <link type="text/css" rel="stylesheet" href="{!URLFOR($Resource.SL_IntranetHub,'/IntranetHub/widgetCSS/allWidgetsCSS.css')}" />
   	<link type="text/css" rel="stylesheet" href="{!URLFOR($Resource.SL_IntranetHub,'/IntranetHub/css/jcf.css')}" />
    <link type="text/css" rel="stylesheet" href="{!URLFOR($Resource.SL_IntranetHub,'/IntranetHub/css/all.css')}" />
    <link type="text/css" rel="stylesheet" href="{!URLFOR($Resource.SL_IntranetHub,'/IntranetHub/css/logo.css')}" />
    <link type="text/css" rel="stylesheet" href="{!URLFOR($Resource.SL_IntranetHub,'/IntranetHub/css/footer.css')}" />
    <link type="text/css" rel="stylesheet" href="{!URLFOR($Resource.SL_IntranetHub,'/IntranetHub/css/media-queries.css')}" />
    <link type="text/css" rel="stylesheet" href="{!URLFOR($Resource.SL_IntranetHub,'/IntranetHub/css/social-feed.css')}" />
    <link type="text/css" rel="stylesheet" href="{!URLFOR($Resource.SL_IntranetHub,'/IntranetHub/css/icons.css')}" />
    <link type="text/css" rel="stylesheet" href="{!URLFOR($Resource.SL_IntranetHub,'/IntranetHub/css/icons-mobile.css')}" />
    <link type="text/css" rel="stylesheet" href="{!URLFOR($Resource.SL_IntranetHub,'/IntranetHub/css/categories.css')}" />
    <!-- End -->
    
    <style>
        @media only screen and (max-width:650px){
            .holder{background:url('{!URLFOR($Resource.SL_IntranetHub,
                      'IntranetHub/images/bg-header-mobile.jpg')}') repeat scroll 0 0 rgba(0, 0, 0, 0);}
        }
    </style>
    
    <script>        
        var __SL_NS = 'SLSI__'; // used by CI, MUST NOT be modified
        
        var communityId = '{!strCommunityId}'; // Used to store community Id when logged in Community.
    
        // all widget instances initialization
        <apex:repeat value="{!allWidgets}" var="widget">
            var widget{!widget.id} = new {!widget.name}('{!widget.id}');
        </apex:repeat> 
    </script>
    
        <apex:composition template="{!strLayout}"> 
            
            <!--------- Start : Header Section ---------->
            <apex:define name="Header">
                
                <div style="width:100%;float:left;">    
                <apex:dynamicComponent id="HeaderUserInfoRegionWidgets" componentValue="{!HeaderUserInfoRegionWidgets}" />   
                </div>        
                <!-- Start - Regions of Header section  -->
                <div id="header" style="width:100%;float:left;"> 
                    
                    <!-- Start - Logo and others widgets  -->
                    <div class="holder" style="position:static;">
                        
                        <!-- logo -->
                        <div class="logoD" style="width:250px;float:left;">
                            <apex:outputpanel rendered="{!if(AND(logoURL != null, logoURL != ''), false, true)}" layout="none"> 
                                <div class="logo" style="background-position: left;">Silverline.com</div>
                            </apex:outputpanel> 
                            <apex:outputpanel rendered="{!if(AND(logoURL != null, logoURL != ''), true, false)}" layout="none"> 
                                <img id="imgIdD" src="{!$Site.Prefix}{!logoURL}" style="max-width:250px;max-height:200px;margin:5px 0px 5px 0px;" />
                            </apex:outputpanel>
                        </div>
                        <!-- End of Logo -->
                        
                        <div id="idHeaderUserInfoRegionWidgets" style="width:750px;float:right;">
                        <apex:dynamicComponent id="HeaderLogoRegionWidgets" componentValue="{!HeaderLogoRegionWidgets}" />                               
                        </div>
                            
                    </div>
                    <!-- Start - Logo and others widgets  -->
                    
                </div>
                <!-- End - Regions of Header section -->
                    
                <!-- Start - Navigation widget  -->   
                <div style="width:100%;float:left;">                  
                <apex:dynamicComponent id="HeaderNavigationRegionWidgets" componentValue="{!HeaderNavigationRegionWidgets}" />
                </div>
                <!-- Start - Navigation widget  -->
                    
            </apex:define>
            <!--------- End : Header Section ------------>
            
            
            <!--------- Start : Banner Region ----------->
            <apex:define name="BannerRegion">
                
                <!-- Start - Banner Region widget -->
            <apex:dynamicComponent id="BannerRegionWidgets" componentValue="{!BannerRegionWidgets}" />
                <!-- End - Banner Region widget -->
                
            </apex:define>
            <!--------- End : Banner Region -------------->
     
     
            <!--------- Start : Left Side Bar ----------->
            <apex:define name="LeftSideBar">
                
                <!-- Start - Left SideBar widgets -->
            <apex:dynamicComponent id="LeftSideBarRegionWidgets" componentValue="{!LeftSideBarRegionWidgets}" />
                <!-- End - Left SideBar widgets -->
                
            </apex:define>
            <!--------- End : Left Side Bar -------------->
            
            
            <!-------- Start : Main section -------------->
            <apex:define name="MainSection">
               
                <!-- Start - Main Section widgets -->
            <apex:dynamicComponent id="MainSectionRegionWidgets" componentValue="{!MainSectionRegionWidgets}" />
                <!-- End -  Main Section widgets -->
                
            </apex:define>
            <!-------- End : Main section --------------->
            
            
            <!-------- Start : Right Side Bar ----------->
            <apex:define name="RightSideBar">   
                    
                <!-- Start - Right SideBar widgets -->    
            <apex:dynamicComponent id="RightSideBarRegionWidgets" componentValue="{!RightSideBarRegionWidgets}" />                  
                <!-- End - Right SideBar widgets -->
                    
            </apex:define>
            <!-------- End : Right Side Bar ------------->
            
            
            <!-- ----------- Mobile view ------------ -->
            <apex:define name="MobileView">  
                
                <div id="header" style="padding:0px;"> 
                    
                    <!-- Start - Logo and others widgets  -->
                    <div class="holder" >  
                        
                        <table width="100%">
                            <tr>
                                <td align="left" width="10%" style="visibility : {!IF(OR($CurrentPage.Parameters.Page == 'Home',$CurrentPage.Parameters.Page == ''), 'hidden', 'visible')};">
                                <!-- class="btn-left" -->
                                    <a href="SLSI__SL_Intranet?page=Home" alt="Go to Home Page">
                                        <img id="imgIdM" src="{!URLFOR($Resource.SL_IntranetHub,'IntranetHub/images/home.png')}" style="max-width:134%; max-height:72px; margin:5px 0px 11px 0px;padding-bottom:1px;" />
                                    </a>
                                </td>
                                
                                <td align="center" width="70%">
                                    <div class="logoM"> <!-- style="width:300px;" -->
                                    <apex:outputpanel rendered="{!if(AND(logoURL != null, logoURL != ''), false, true)}" layout="none"> 
                                            <div class="logo" style="background-position: center;width:100%;">Silverline.com</div>
                                        </apex:outputpanel> 
                                    <apex:outputpanel rendered="{!if(AND(logoURL != null, logoURL != ''), true, false)}" layout="none"> 
                                            <img id="imgIdM" src="{!logoURL}" style="max-width:95%; max-height:72px; " /> <!-- margin:5px 0px 5px 0px; -->
                                        </apex:outputpanel>
                                    </div>
                                    <!-- End of Logo -->
                                </td>
                                <td align="right" width="20%">
                                    <div align="right"><!-- style="padding-right:5px;" -->
    <!--                                    <img alt="Log out" src="{!URLFOR($Resource.SL_IntranetHub,'/IntranetHub/images/logout.ico')}" /> -->
                                        <strong style="color: #5F6065;font-size: 30px;line-height: 33px;">
                                            <a style="color: #5F6065;cursor:pointer;display:'{!IF(ISPICKVAL($User.UserType,'Guest'), none, block)}'" onclick= "window.location.href= '{!$Site.Prefix}/secur/logout.jsp'; ">
                                                <!-- Log --><!-- {!$Label.site.logout} -->
                                                <img style="width:60%;max-height: 65px;max-width: 134%;" src="{!URLFOR($Resource.SL_IntranetHub, '/IntranetHub/images/logout.png')}" alt="Smiley face"/>
                                            </a>            
                                        </strong>
                                    </div>
                               </td>
                            </tr>
                        </table>
                        
                    </div>
                 </div>
                    
            <apex:dynamicComponent id="MobileViewWidgets" componentValue="{!MobileViewWidgets}" />    
                              
            </apex:define>
            <!-- ----------- Mobile view ------------ -->
            
            <!-- Footer section -->
		    <apex:define name="Footer" >
			    <apex:outputPanel rendered="{!$Setup.SLSI__SL_CustomizableComponent__c.SLSI__Footer__c == ''}" layout="none">     
				    <p>Copyright &copy;2013-2014 Silverline. All rights reserved.</p>
			    </apex:outputPanel>
			    <apex:outputPanel rendered="{!$Setup.SLSI__SL_CustomizableComponent__c.SLSI__Footer__c != ''}" layout="none">     
				    <p>{!$Setup.SLSI__SL_CustomizableComponent__c.SLSI__Footer__c}</p>
			    </apex:outputPanel>
		    </apex:define>
            <!--END of Footer section -->
            
        </apex:composition>
    
    <script>
        hideAddressBar1();
        
        $(document).ready(function() 
        {
            resizeLogo();
        });
        
        $(window).load(function()
        {
            resizeLogo();
        });
        
        function resizeLogo()
        {
            if('{!JSENCODE(logoURL)}' != '')
            { 
                var imgH1 = 79;
                var h1 = $('#imgIdD').height();
                if(h1 < imgH1 && h1 != 0)
                    $('#imgIdD').css('margin-top', (imgH1 - h1)/2);
            }
        }
    </script>
    
</apex:page>