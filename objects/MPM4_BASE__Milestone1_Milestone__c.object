    <validationRules>
        <fullName>MPM4_BASE__Alias_must_not_contain_spaces</fullName>
        <active>true</active>
        <errorConditionFormula>CONTAINS(TRIM(MPM4_BASE__Alias__c ),&quot; &quot;)</errorConditionFormula>
        <errorDisplayField>MPM4_BASE__Alias__c</errorDisplayField>
        <errorMessage>The Milestone Alias cannot contain spaces.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MPM4_BASE__CanNotAddSubmilestoneToParentCompleted</fullName>
        <active>true</active>
        <errorConditionFormula>MPM4_BASE__Parent_Milestone__r.MPM4_BASE__Complete__c</errorConditionFormula>
        <errorDisplayField>MPM4_BASE__Parent_Milestone__c</errorDisplayField>
        <errorMessage>You can not add milestones to parent milestones with status completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MPM4_BASE__CannotBeCompleteWithOpenTasks</fullName>
        <active>false</active>
        <errorConditionFormula>false</errorConditionFormula>
        <errorDisplayField>MPM4_BASE__Complete__c</errorDisplayField>
        <errorMessage>You cannot mark a milestone as complete if it has open tasks.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MPM4_BASE__Deadline_within_project_boundaries</fullName>
        <active>false</active>
        <errorConditionFormula>if ( and (MPM4_BASE__Deadline__c &lt;&gt; null, MPM4_BASE__Project__r.MPM4_BASE__Deadline__c &lt;&gt;  null, MPM4_BASE__Deadline__c &gt; MPM4_BASE__Project__r.MPM4_BASE__Deadline__c) , true, false)</errorConditionFormula>
        <errorDisplayField>MPM4_BASE__Deadline__c</errorDisplayField>
        <errorMessage>Deadline must be less than or equal to the project deadline.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MPM4_BASE__KickoffDeadlineWithinParentBoundaries</fullName>
        <active>false</active>
        <errorConditionFormula>(NOT(ISNULL(MPM4_BASE__Kickoff__c)) &amp;&amp; NOT(ISNULL(MPM4_BASE__Parent_Milestone__r.MPM4_BASE__Kickoff__c)) &amp;&amp; MPM4_BASE__Kickoff__c &lt; MPM4_BASE__Parent_Milestone__r.MPM4_BASE__Kickoff__c) || (NOT(ISNULL(MPM4_BASE__Deadline__c)) &amp;&amp; NOT(ISNULL(MPM4_BASE__Parent_Milestone__r.MPM4_BASE__Deadline__c)) &amp;&amp; MPM4_BASE__Deadline__c &gt; MPM4_BASE__Parent_Milestone__r.MPM4_BASE__Deadline__c)</errorConditionFormula>
        <errorDisplayField>MPM4_BASE__Kickoff__c</errorDisplayField>
        <errorMessage>Milestone Kickoff must not be earlier than Parent Kickoff and Milestone Deadline must not be later than Parent Deadline.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MPM4_BASE__Kickoff_before_deadline</fullName>
        <active>false</active>
        <description>Kickoff must be less or equal than the deadline</description>
        <errorConditionFormula>NOT(ISNULL(MPM4_BASE__Kickoff__c)) &amp;&amp; NOT(ISNULL(MPM4_BASE__Deadline__c)) &amp;&amp;  MPM4_BASE__Deadline__c &lt; MPM4_BASE__Kickoff__c</errorConditionFormula>
        <errorDisplayField>MPM4_BASE__Kickoff__c</errorDisplayField>
        <errorMessage>Kickoff must be less or equal than the deadline</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MPM4_BASE__Milestone_between_Project_duration</fullName>
        <active>false</active>
        <description>Milestone kickoff must be grater or equal than project kickoff and milestone deadline must be less or equal than project deadline</description>
        <errorConditionFormula>(NOT(ISNULL(MPM4_BASE__Kickoff__c)) &amp;&amp; NOT(ISNULL(MPM4_BASE__Project__r.MPM4_BASE__Kickoff__c )) &amp;&amp; MPM4_BASE__Kickoff__c&lt; MPM4_BASE__Project__r.MPM4_BASE__Kickoff__c ) 
|| 
(NOT(ISNULL(MPM4_BASE__Deadline__c)) &amp;&amp; NOT(ISNULL(MPM4_BASE__Project__r.MPM4_BASE__Deadline__c)) &amp;&amp; MPM4_BASE__Deadline__c &gt; MPM4_BASE__Project__r.MPM4_BASE__Deadline__c)</errorConditionFormula>
        <errorMessage>Milestone kickoff must be grater or equal than project kickoff and milestone deadline must be less or equal than project deadline</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MPM4_BASE__Milestone_must_have_parent_record</fullName>
        <active>true</active>
        <description>Milestone records must always have a parent Project and/or a parent Milestone</description>
        <errorConditionFormula>ISBLANK(MPM4_BASE__Project__c) &amp;&amp; ISBLANK(MPM4_BASE__Parent_Milestone__c)</errorConditionFormula>
        <errorMessage>Please select a parent Project, a parent Milestone, or both.</errorMessage>
    </validationRules>
