    <validationRules>
        <fullName>Cannot_Change_Account_Name_Once_Saved</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
$Profile.Name = &quot;System Administrator&quot; , FALSE ,
AND (ISCHANGED( Account_Name__c ),
NOT (IsNull(Account_Name__c ))))</errorConditionFormula>
        <errorDisplayField>Account_Name__c</errorDisplayField>
        <errorMessage>Account Name cannot be changed once the Contract has been saved</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contract_ID_Equals_Numbers</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
$Profile.Name = &quot;System Administrator&quot; , FALSE ,
AND (
 OR (
   NOT(IsNumber( Contract_ID__c )),
   AND(ISCHANGED(  Contract_ID__c),
       NOT (IsNull ( Contract_ID__c))))))</errorConditionFormula>
        <errorDisplayField>Contract_ID__c</errorDisplayField>
        <errorMessage>Contract ID must be a string of 5 numbers and cannot be changed after saved.</errorMessage>
    </validationRules>
