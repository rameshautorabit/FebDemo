    <validationRules>
        <fullName>Approval_by_Administrator_Only</fullName>
        <active>true</active>
        <description>Only top-level administrators may sign-off on requests.</description>
        <errorConditionFormula>AND(ISCHANGED(  Administrative_Approval__c  ),  Administrative_Approval__c   = TRUE, OR( LastModifiedBy.Profile.Name  &lt;&gt; "System Administrator",  LastModifiedBy.Profile.Name  &lt;&gt; "Delegated Admin",  LastModifiedBy.Profile.Name  &lt;&gt; "Delta Technology"))</errorConditionFormula>
        <errorDisplayField>Administrative_Approval__c</errorDisplayField>
        <errorMessage>Only System Administrators can give Administrative Approval</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Business_Priority</fullName>
        <active>true</active>
        <errorConditionFormula>OR(Business_Priority__c &gt; 10,Business_Priority__c = 0)</errorConditionFormula>
        <errorDisplayField>Business_Priority__c</errorDisplayField>
        <errorMessage>Enter a Priority between 1 &amp; 10.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Release_Date_Validation</fullName>
        <active>true</active>
        <description>Release Date cannot be an earlier date than start date</description>
        <errorConditionFormula>Start_Date__c &gt;  Release_Date__c</errorConditionFormula>
        <errorDisplayField>Release_Date__c</errorDisplayField>
        <errorMessage>Release Date cannot be an earlier date than start date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SFDC_Priority</fullName>
        <active>true</active>
        <errorConditionFormula>OR(AND($Profile.Id &lt;&gt; '00e70000000s6G2',  SFDC_Priority__c &gt;= 0),   AND(SFDC_Priority__c &gt; 50,SFDC_Priority__c = 0))</errorConditionFormula>
        <errorDisplayField>SFDC_Priority__c</errorDisplayField>
        <errorMessage>Enter a value between 1-50. NOTE: Only System Administrators can update this field.</errorMessage>
    </validationRules>
